// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

// Custom generator for kysely typegen
generator kysely {
  provider = "prisma-kysely"
  output   = "../src/types"
  fileName = "types.ts"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.db"
}



